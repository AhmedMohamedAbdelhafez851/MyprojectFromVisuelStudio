    <!--section start-->
    <section class="register-page section-b-space">
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <h3>create account</h3>
                    <div class="theme-card">
                        <form class="theme-form"
                        method="post" asp-controller="users"
                        asp-action="register">
                        
                            <div class="form-row">
                                <div asp-validation-summary="All"> 

                                </div>
                                <div class="col-md-6">
                                    <label for="email">first name</label>
                                    <input asp-for = "FirstName"  type="text" class="form-control" id="fname" placeholder="first name"
                                        required="">
                                        <span asp-validation-for="FirstName"></span>
                                     
                                </div>
                                <div class="col-md-6">
                                    <label for="review">last name</label>
                                    <input asp-for="LastName" type="text" class="form-control" id="lname" placeholder="last name"
                                        required="">
                                        <span asp-validation-for="LastName"></span>
                                </div>
                            </div>
                            <div class="form-row">
                                <div class="col-md-6">
                                    <label for="email">email</label>
                                    <input asp-for="Email" type="text" class="form-control" id="email" placeholder="email" required="">
                                    <span asp-validation-for="Email"></span>
                                </div>
                                <div class="col-md-6">
                                    <label for="review">password</label>
                                    <input asp-for="Password" type="password" class="form-control" id="review"
                                        placeholder="enter your password" required />
                                        <span asp-validation-for="Password"></span>
                                </div><button type="submit" class="btn btn-solid">create account</button>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </section>
